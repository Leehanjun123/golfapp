{
  "cli": {
    "version": ">= 5.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "apk"
      }
    },
    "production": {
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Release"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_API_URL": "https://api.golfaicoach.com"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "REPLACE_WITH_ACTUAL_APPLE_ID",
        "ascAppId": "REPLACE_WITH_ACTUAL_ASC_APP_ID",
        "appleTeamId": "REPLACE_WITH_ACTUAL_APPLE_TEAM_ID"
      },
      "android": {
        "serviceAccountKeyPath": "REPLACE_WITH_ACTUAL_SERVICE_ACCOUNT_KEY_PATH",
        "track": "production"
      }
    }
  }
}